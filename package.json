{
  "name": "bluesquare-assistant-backend",
  "version": "1.0.0",
  "description": "OAuth backend for BlueSquare Assistant Chrome extension",
  "main": "src/server.js",
  "scripts": {
    "dev": "node src/server.js",
    "start": "node src/server.js",
    "studio": "langgraph dev",
    "studio:tunnel": "langgraph dev --tunnel",
    "test:setup": "node tests/local/setupTestSession.js",
    "test": "node tests/local/testClient.js",
    "test:approvals": "node tests/local/testClientWithApprovals.js",
    "test:memory": "node tests/local/testMemorySimple.js",
    "test:memory:rolling": "node tests/local/testRollingMemoryWithApprovals.js",
    "test:memory:10": "node tests/local/testRollingMemory10Queries.js",
    "test:reset": "node tests/local/resetTestConversation.js",
    "test:logs": "node tests/local/testWithLogs.js",
    "test:analyze": "node tests/local/analyzeLogs.js summary",
    "test:report": "node tests/local/analyzeLogs.js report",
    "test:verify": "node tests/local/analyzeLogs.js verify"
  },
  "dependencies": {
    "@langchain/core": "0.3.72",
    "@langchain/langgraph": "^0.2.25",
    "@langchain/langgraph-checkpoint-postgres": "^0.1.2",
    "@langchain/openai": "0.6.9",
    "@supabase/supabase-js": "^2.39.0",
    "axios": "^1.6.2",
    "chrono-node": "^2.8.4",
    "cors": "^2.8.5",
    "cross-fetch": "^4.1.0",
    "dayjs": "^1.11.18",
    "express": "^4.18.2",
    "langchain": "0.3.31",
    "langsmith": "^0.2.14",
    "mem0ai": "^2.1.38",
    "pg": "^8.11.3",
    "uuid": "^12.0.0",
    "ws": "^8.18.3",
    "zod": "^3.23.8"
  },
  "overrides": {
    "@langchain/core": "0.3.72"
  },
  "engines": {
    "node": "20.x"
  },
  "devDependencies": {
    "@langchain/langgraph-cli": "^0.0.69",
    "canvas": "^3.2.0",
    "dotenv": "^17.2.1"
  }
